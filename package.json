{
  "name": "survey",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron-main.js",
  "scripts": {
    "dev:ui": "vite",
    "dev:electron": "wait-on -t 30000 http://localhost:5173 && NODE_ENV=development electron .",
    "dev": "concurrently -k \"npm:dev:ui\" \"npm:dev:electron\"",
    "build": "vite build",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@supabase/supabase-js": "^2.81.1",
    "martinez-polygon-clipping": "^0.7.4",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^3.11.174",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^2.2.1",
    "xlsx": "^0.18.5",
    "xlsx-js-style": "^1.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^25.2.1",
    "electron-builder": "^24.8.0",
    "events": "^3.3.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.5",
    "vite": "^5.2.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "wait-on": "^7.0.1"
  },
  "build": {
    "appId": "com.example.survey",
    "files": [
      "dist/**/*",
      "electron-main.js",
      "preload.js",
      "package.json"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    }
  }
}
